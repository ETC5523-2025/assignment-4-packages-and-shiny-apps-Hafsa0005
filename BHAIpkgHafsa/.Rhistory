#' }
launch_haiexplorer <- function() {
app_dir <- system.file("shiny", package = "BHAIpkgHafsa")
if (app_dir == "") {
stop("Could not find the Shiny app directory. Please reinstall the BHAIpkgHafsa package.",
call. = FALSE)
}
shiny::runApp(app_dir, display.mode = "normal")
